            <moqui.basic.StatusFlowTransition statusFlowId="Default" statusId="PtjProcessingStopped" toStatusId="PtjCanceled" transitionName="Cancel"/>
        </seed-data>
        <master><detail relationship="printer"/><detail relationship="status"/></master>
    </entity>
</entities>
