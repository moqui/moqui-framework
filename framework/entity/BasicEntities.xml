        <master>
            <detail relationship="status"/><detail relationship="type"/>
            <detail relationship="root" use-master="default"/><detail relationship="parent" use-master="default"/>
            <detail relationship="children" use-master="default"/>
            <detail relationship="fromUser"/><detail relationship="toUser"/>
            <detail relationship="template"/><detail relationship="server"/>
        </master>
